pipeline {
  agent {
    node {
      label 'DEPLOY_TEST_LAMBDA'
    }
  }
  stages {
    stage('Deploy') {
      steps {
        script {
          def depScript = load ('deploy/deploy_script.groovy')
          depScript.deployOnLAMBDA();
         }
       }
     }
   stage('Pausing') {
            steps {
               script {
               def test() {
               echo "Start"
               sleep(5)
               echo "Stop"
               }             
            }
        }
   }
 }    
}	
